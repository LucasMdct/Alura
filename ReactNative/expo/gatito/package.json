    "expo": "~49.0.15",
    "expo-status-bar": "~1.6.0",
    "intl": "^1.2.5",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.6",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react-native": "^12.4.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
